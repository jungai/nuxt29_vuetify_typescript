{
  "name": "nuxt29_vuetify_typescript",
  "version": "0.2.0",
  "description": "plain setup nuxtjs 2.9 (with vuetify and typescript) project",
  "author": "kpping",
  "license": "MIT",
  "scripts": {
    "analysis": "yarn cp:localhost && nuxt-ts build -a",
    "cp:localhost": "cpy env/localhost.ts configs --rename=env.ts",
    "dev": "cross-env HOST=0.0.0.0 PORT=8080 nuxt-ts",
    "dev:localhost": "yarn cp:localhost && yarn dev",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.2.1",
    "intersection-observer": "^0.7.0",
    "ky": "^0.15.0",
    "ky-universal": "^0.3.0",
    "nuxt": "^2.10.1",
    "nuxt-property-decorator": "^2.4.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.3.3",
    "@mdi/font": "^4.5.95",
    "@nuxt/types": "^0.3.1",
    "@nuxt/typescript-build": "^0.3.1",
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/vuetify": "^1.9.0",
    "@types/jest": "^24.0.19",
    "@types/node": "^10",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-jest": "^24.9.0",
    "cpy-cli": "^2.0.0",
    "cross-env": "^6.0.3",
    "deepmerge": "^4.1.1",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-nuxt": "^0.4.3",
    "eslint-plugin-prettier": "^3.1.0",
    "fibers": "^4.0.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "sass": "^1.23.0",
    "sass-loader": "^8.0.0",
    "vue-jest": "^4.0.0-beta.2",
    "vuetify": "^2.1.4"
  }
}
